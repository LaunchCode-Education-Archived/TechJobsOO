<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@foreach (var job in ViewBag.jobs)
{
    <table class="job-listing">
        <tr>
            <td>Name</td>
            <td>@job.Name</td>
        </tr>
        <tr>
            <td>Employer</td>
            <td>@job.Employer</td>
        </tr>
        <tr>
            <td>Location</td>
            <td>@job.Location</td>
        </tr>
        <tr>
            <td>Skill</td>
            <td>@job.CoreCompetency</td>
        </tr>
        <tr>
            <td>Position Type</td>
            <td>@job.PositionType</td>
        </tr>
    </table>
}